{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ".output/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
